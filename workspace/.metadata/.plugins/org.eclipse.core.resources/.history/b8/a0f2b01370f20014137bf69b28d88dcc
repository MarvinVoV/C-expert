OBJS = selectserver.o gen.o
CC = gcc
CFLAGS = -Wall -O -g 
INCLUDE = -I../include


server: $(OBJS)
	$(CC) $(CFLAGS) $(INCLUDE) $(OBJS) -o $@
selectserver.o: gen.o
	$(CC) $(CFLAGS) $(INCLUDE) -c selectserver.c gen.o -o $@
gen.o: gen.c 
	$(CC) $(CFLAGS) $(INCLUDE) -c gen.c -o $@
hello2.o:hello2.c 
	$(CC) $(CFLAGS) $(INCLUDE) -c hello2.c -o $@

.PHONY:clean help
clean:
	rm -rf *.o hello
help:
	@echo You must be root to make